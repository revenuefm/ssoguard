{
    "name": "revenuefm/ssoguard",
    "type": "library",
    "description": "Laravel SSO package",
    "keywords": [
        "SSO",
        "Laravel",
        "oAuth"
    ],
    "homepage": "https://github.com/revenuefm/ssoguard",
    "license": "MIT",
    "authors": [
        {
            "name": "RevenueFM",
            "email": "support@revenue.fm",
            "homepage": "https://revenue.fm",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Revenuefm\\Ssoguard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Revenuefm\\Ssoguard\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Revenuefm\\Ssoguard\\SsoguardServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
